# this *must* be before the cz/rz catch-all
Host cz-stash cz-stash.llnl.gov cz-bitbucket cz-bitbucket.llnl.gov
    HostName cz-bitbucket.llnl.gov
    Identityfile ~/.ssh/keys/llnl
    User git
    Port 7999
    ProxyCommand ssh herbein1@rzgw -W %h:%p

Host hype ipa cab sierra catalyst vulcan aztec quartz oslic hype2 surface rz* cz*
     User herbein1
     Identityfile ~/.ssh/keys/llnl
     ForwardX11 yes
     ForwardX11Trusted yes
     ForwardAgent yes

Host czgw
     Hostname hype.llnl.gov
     ProxyCommand None

Host rzgw
     Hostname %h.llnl.gov
     ProxyCommand None

Host hype ipa cab sierra catalyst vulcan aztec quartz oslic hype2 surface rz* cz*
     Hostname %h.llnl.gov
     ProxyCommand bash ~/.ssh/connect_if_gw_established.sh %h rzgw --port 622

