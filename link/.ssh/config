Host *
     ServerAliveInterval 60
     Compression yes
     KeepAlive yes
     ControlMaster auto
     ControlPath /tmp/ssh-%r@%h:%p
     IdentitiesOnly yes
     PreferredAuthentications publickey,keyboard-interactive,password,hostbased,gssapi-with-mic
     ForwardX11Timeout 1d

Include LOCAL_config

###################
#UDel
###################
Host *udel.edu
     User sherbein
     Identityfile ~/.ssh/keys/udel

Host udgw
     HostName catan.eecis.udel.edu
     Identityfile ~/.ssh/keys/udel
     ProxyCommand None

Host ratbert stimpy hoek catan
     Identityfile ~/.ssh/keys/udel
     Proxycommand ssh -q udgw nc %h.eecis.udel.edu 22

Host yankees orioles mlb
     Identityfile ~/.ssh/keys/udel
     Proxycommand ssh -q udgw nc %h.acad.ece.udel.edu 22

