Host *
     ServerAliveInterval 60
     CompressionLevel 8
     Compression yes
     KeepAlive yes
     ControlMaster auto
     ControlPath /tmp/ssh-%r@%h:%p
     IdentitiesOnly yes
     PreferredAuthentications publickey,keyboard-interactive,password,hostbased,gssapi-with-mic

